<ImageScreen>:
    name: 'image_screen'
    FloatLayout:
        size_hint: None, None

    DPEAButton:
        id: back_button
        text: 'back'
        size: 100, 100
        font_size: 30
        x: root.width * 0.05
        y: root.height * 0.8
        color: 0, 1, 0, 1
        on_press: root.imageBack()
